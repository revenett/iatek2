    <div id="masthead">  
      <div class="container">
          <div class="row">
            <div class="col-sm-8">
              <h1>Bootstrap Templates <p class="lead"><span style="color:#CCFF33;">+Free</span> Themes for Bootstrap</p></h1>
            </div>
            <div class="col-sm-4">
                <!-- ad space
                <div class="well well-lg"> 
                  <div class="row">
                    <div class="col-sm-12">
                      <img class="img-responsive" src="//placehold.it/280x100">
                    </div>
                  </div>
                </div>
                -->
                <p class="text-right">For Bootstrap Designers and Developers</p>
            </div>
          </div>
      </div><!-- /cont -->
    </div>
    
    <div class="container" id="main">
	    <div class="row" id="starters">
      		<div class="col-md-12">
      			<h2>Starter Templates</h2>
                <p>Kickstart your Bootstrap design with these clean, vanilla templates.</p>
                <hr>
      		</div>
      	</div>
      	<div class="row">
      	    <% for(i in starters) {%>
  			<div class="col-sm-4">
              	<div class="panel panel-default">
                  	<div class="panel-heading"><h3><a href="/bootstrap-template/<%=starters[i].title.toLowerCase().replace(" ","-")%>"><%=starters[i].title%></h3></a></div>
                	<div class="panel-body"><a href="/templates/<%=starters[i].id%>"><img src="<%=starters[i].imgUrl%>" class="img-responsive" /></a></div>
                	<div class="panel-footer"><strong><%=starters[i].short%></strong></div>
                </div>              
      		</div>
      		<% } %>
        </div>
        <hr>
        <div class="row" id="custom">
      		<div class="col-md-12">
      			<h2>Templates with Themes</h2>
                <p>Get a less Bootstrap-y look with these fresh custom themes</p>
                <hr>
      		</div>
      	</div>
      	<div class="row">
      	    <% for(i in themes) {%>
  			<div class="col-sm-4">
              	<div class="panel panel-default">
                  	<div class="panel-heading"><h3><%=themes[i].title%></h3></div>
                	<div class="panel-body"><a href="/templates/<%=themes[i].id%>"><img src="<%=themes[i].imgUrl%>" class="img-responsive" /></a></div>
                	<div class="panel-footer"><strong><%=themes[i].short%></strong></div>
                </div>              
      		</div>
      		<% } %>
        </div>
    </div>
    
