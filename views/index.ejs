                    <a href="/bootstrap-templates/slider" class="label label-default" target="ext">slider</a> 
                    
                    <a href="/bootstrap-templates/gallery" class="label label-default" target="ext">gallery</a> 
                    
                    <a href="/bootstrap-templates/navbar" class="label label-default" target="ext">navbar</a> 
                    
